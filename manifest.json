{
  "description": "MacOS inspired Mojave theme that changes based on time.",
  "homepage_url": "https://github.com/karanvirheer/Firefox-Mojave-Theme",
  "manifest_version": 2,
  "name": "MacOS Mojave Dynamic Theme",
  "permissions": ["alarms", "theme"],
  "background": {
    "scripts": ["background.js"]
  },
  "version": "1.0",
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "55.0a2"
    }
  }
}
